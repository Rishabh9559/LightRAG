  <data key="d4">unknown_source</data>
</node>
<node id="West Africa">
  <data key="d0">West Africa</data>
  <data key="d1">geo</data>
  <data key="d2">A region where HIV-2 is chiefly found in patients.&lt;SEP&gt;West Africa is a region where HIV-2 is chiefly found in patients.</data>
